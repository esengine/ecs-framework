{
  "id": "particle",
  "name": "@esengine/particle",
  "globalKey": "particle",
  "displayName": "Particle System",
  "description": "2D particle system for visual effects | 2D 粒子系统用于视觉特效",
  "version": "1.0.0",
  "category": "Rendering",
  "icon": "Sparkles",
  "tags": [
    "2d",
    "particle",
    "effects",
    "vfx"
  ],
  "isCore": false,
